var oneToTenArray = [];
for (var value = 1; value <= 10; value++) {
  oneToTenArray.push(value);
}
for (var index = 0; index < oneToTenArray.length; index++) {
  console.log(oneToTenArray[index]);
}
// If the number is a prime echo: 'FiZZBUZZ++'
// console.log(prime(10, true));
// If the number is divisible by 3 the script should echo: 'FIZZ'

for (var i = 0; i < oneToTenArray.length; i++) {
  if (oneToTenArray[i] % 3 == 0) {
    console.log("fizz");
  }
}
// If the number is divisible by 5 the script should echo: 'BUZZ'
for (var i = 0; i < oneToTenArray.length; i++) {
  if (oneToTenArray[i] % 5 == 0) {
    console.log("buzz");
  }
}
